{
  "C_Cpp.intelliSenseEngine": "Default",
  "files.associations": {
    "*.md": "markdown",
    ".nycrc": "json",
    "*.yaml": "yaml",
    "test.h": "c",
    "system_error": "cpp",
    "logger.h": "c",
    "platform.h": "c",
    "game_types.h": "c",
    "application.h": "c",
    "chrono": "cpp",
    "variant": "cpp",
    "input.h": "c",
    "*.rh": "c",
    "cstddef": "c",
    "limits": "c",
    "version": "c",
    "*.tcc": "c",
    "cstdio": "c"
  },
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    {
      "text": "TODO:",
      "color": "red",
      "backgroundColor": "rgba(0,0,0,0)"
    },
    {
      "text": "HACK:",
      "color": "#ff6600",
      "backgroundColor": "rgba(0,0,0,0)"
    },
    {
      "text": "NOTE:",
      "color": "green",
      "backgroundColor": "rgba(0,0,0,0)"
    }
  ],
  "todohighlight.defaultStyle": {
    "color": "red",
    "backgroundColor": "#ffab00",
    "overviewRulerColor": "rgba(0,0,0,0)",
    "isWholeLine": false
  }
}
